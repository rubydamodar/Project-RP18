        lpAuth = window.open('/proper/login/ajaxAuth', "lpAuth", "width=450,height=850,menubar=no,resizable=no,status=no,location=no");
    }

    function authenticated() {
        if(lpAuth) {
            lpAuth.close();
        }
        passwordVerified();
    }
